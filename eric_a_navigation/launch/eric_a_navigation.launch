  <!-- move_base -->
  <include file="$(find eric_a_navigation)/launch/move_base.launch">
    <arg name="machine" default="$(arg machine)"/>
    <arg name="move_forward_only" value="$(arg move_forward_only)"/>
  </include>

  <!-- rviz -->
