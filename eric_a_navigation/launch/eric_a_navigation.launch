<launch>
  <!-- Arguments -->
  <arg name="map_file" default="/home/ubuntu/catkin_ws/src/ERIC_A/eric_a_slam/map/jeonghan2.yaml"/>
  <arg name="open_rviz" default="false"/>
  <arg name="move_forward_only" default="false"/>
