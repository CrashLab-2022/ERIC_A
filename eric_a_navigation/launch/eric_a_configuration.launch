<launch>
    <arg name="machine" />
    <!-- ekf_localization -->
    <node pkg="robot_localization" type="ekf_localization_node" name="ekf_localization" clear_params="true" machine="$(arg machine)"> 
        <rosparam command="load" file="$(find eric_a_navigation)/config/ekf_localization.yaml" />
    </node>

    <node pkg="eric_a_naviagtion" type="init_pose.py" name="eric_a_init_pose" machine="$(arg machine)"/>
</launch>
